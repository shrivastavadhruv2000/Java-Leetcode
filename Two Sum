class Solution {
    public int[] twoSum(int[] nums, int target) {
        int[] res = new int[2];
        int s=target;
        for(int i=0;i<nums.length;i++){
            s=target-nums[i];
            if(s==nums[i+1]){
                res[0]=i;
                res[1]=i+1;
                return res;
            }
        }
       return res;
    }
}
